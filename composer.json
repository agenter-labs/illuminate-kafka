{
    "name": "agenter-labs/illuminate-kafka",
    "description": "Kafka manager Laravel",
    "keywords": [
        "laravel",
        "kafka"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Abdulla",
            "email": "dev@agenter.com",
            "homepage": "https://agenter.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^8.0",
        "illuminate/contracts": "^8.0",
        "illuminate/cache": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
    "psr-4": {
            "AgenterLab\\Kafka\\": "./src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AgenterLab\\Kafka\\KafkaServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
